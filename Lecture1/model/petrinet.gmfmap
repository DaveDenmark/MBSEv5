<?xml version="1.0" encoding="UTF-8"?>
<gmfmap:Mapping
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
    xmlns:gmfmap="http://www.eclipse.org/gmf/2008/mappings"
    xmlns:gmftool="http://www.eclipse.org/gmf/2005/ToolDefinition">
  <nodes>
    <containmentFeature
        href="petrinet.ecore#//Petrinet/nodes"/>
    <ownedChild>
      <domainMetaElement
          href="petrinet.ecore#//Place"/>
      <labelMappings
          xsi:type="gmfmap:FeatureLabelMapping">
        <diagramLabel
            href="petrinet.gmfgraph#PlaceName"/>
        <features
            href="petrinet.ecore#//Node/name"/>
      </labelMappings>
      <tool
          xsi:type="gmftool:CreationTool"
          href="petrinet.gmftool#//@palette/@tools.0/@tools.2"/>
      <diagramNode
          href="petrinet.gmfgraph#Place"/>
      <children>
        <containmentFeature
            href="petrinet.ecore#//Place/tokens"/>
        <ownedChild>
          <domainMetaElement
              href="petrinet.ecore#//Token"/>
          <tool
              xsi:type="gmftool:CreationTool"
              href="petrinet.gmftool#//@palette/@tools.0/@tools.3"/>
          <diagramNode
              href="petrinet.gmfgraph#Token"/>
        </ownedChild>
      </children>
    </ownedChild>
  </nodes>
  <nodes>
    <containmentFeature
        href="petrinet.ecore#//Petrinet/nodes"/>
    <ownedChild>
      <domainMetaElement
          href="petrinet.ecore#//Transition"/>
      <labelMappings
          xsi:type="gmfmap:FeatureLabelMapping">
        <diagramLabel
            href="petrinet.gmfgraph#TransitionName"/>
        <features
            href="petrinet.ecore#//Node/name"/>
      </labelMappings>
      <tool
          xsi:type="gmftool:CreationTool"
          href="petrinet.gmftool#//@palette/@tools.0/@tools.1"/>
      <diagramNode
          href="petrinet.gmfgraph#Transition"/>
    </ownedChild>
  </nodes>
  <links>
    <domainMetaElement
        href="petrinet.ecore#//Arc"/>
    <containmentFeature
        href="petrinet.ecore#//Petrinet/arcs"/>
    <tool
        xsi:type="gmftool:CreationTool"
        href="petrinet.gmftool#//@palette/@tools.0/@tools.0"/>
    <diagramLink
        href="petrinet.gmfgraph#Arc"/>
    <sourceMetaFeature
        xsi:type="ecore:EReference"
        href="petrinet.ecore#//Arc/source"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="petrinet.ecore#//Arc/target"/>
    <creationConstraints>
      <sourceEnd
          body="(self.oclIsKindOf(Place) and oppositeEnd.oclIsKindOf(Transition) ) or &#xD;&#xA;(self.oclIsKindOf(Transition) and oppositeEnd.oclIsKindOf(Place) )"/>
    </creationConstraints>
  </links>
  <diagram>
    <diagramCanvas
        href="petrinet.gmfgraph#petrinet"/>
    <domainModel
        href="petrinet.ecore#/"/>
    <domainMetaElement
        href="petrinet.ecore#//Petrinet"/>
    <palette
        href="petrinet.gmftool#//@palette"/>
  </diagram>
</gmfmap:Mapping>
